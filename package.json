    "dayjs": "^1.11.9",
    "echarts": "^5.4.3",
    "element-plus": "^2.3.12",
    "http-server": "^14.1.1",
    "pinia": "^2.1.6",
    "postcss": "^8.4.28",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.3.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "xlsx": "^0.18.5"
